{
  "name": "vue-hoc",
  "version": "0.1.6",
  "description": "Create Higher Order Vue Components",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run build && ava",
    "coverage": "nyc --reporter=html npm test",
    "debug": "inspect node_modules/ava/profile",
    "build": "babel src/ -d dist/ --source-maps",
    "flow": "flow",
    "eslint": "eslint src --fix",
    "lint": "npm run eslint && npm run flow"
  },
  "ava": {
    "files": [
      "spec/**/*.spec.js"
    ],
    "source": [
      "dist/**/*.js"
    ],
    "require": [
      "./spec/hooks.js"
    ],
    "concurrency": 8
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackmellis/vue-hoc.git"
  },
  "author": "Jack Ellis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackmellis/vue-hoc/issues"
  },
  "homepage": "https://github.com/jackmellis/vue-hoc#readme",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "browser-env": "^3.1.2",
    "eslint": "^4.5.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.53.0",
    "inspect-process": "^0.5.0",
    "nyc": "^11.1.0",
    "sinon": "^3.2.0",
    "vue": "^2.4.4",
    "vuenit": "^1.1.0"
  },
  "peerDependencies": {
    "vue": "^2.4.0"
  }
}
